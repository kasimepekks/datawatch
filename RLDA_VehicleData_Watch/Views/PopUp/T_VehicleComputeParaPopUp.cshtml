@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>车辆参数副表-数据分析</title>
    <link href="~/layui/css/layui.css" rel="stylesheet" />
    <script src="~/layui/layui.js"></script>

</head>
<body>
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>车辆信息</legend>
    </fieldset>
    <form class="layui-form" action="">
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">车辆号</label>
                <div class="layui-input-inline">
                    <input type="text" name="vehicleid" id="vehicleid" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">油门容忍点数</label>
                <div class="layui-input-inline">
                    <input type="text" id="throttlelastingpoints" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>

        </div>

        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">最小轴距</label>
                <div class="layui-input-inline">
                    <input type="text" id="wheelbaselower" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">最大轴距</label>
                <div class="layui-input-inline">
                    <input type="text" id="wheelbaseupper" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">冲击清零标准值</label>
                <div class="layui-input-inline">
                    <input type="text" id="bumpzerostandard" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">冲击容忍最大速度</label>
                <div class="layui-input-inline">
                    <input type="text" id="bumpmaxspeed" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">加速度容忍落差值</label>
                <div class="layui-input-inline">
                    <input type="text" id="accvaluegap" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">加速度容忍落差点数</label>
                <div class="layui-input-inline">
                    <input type="text" id="acctimegap" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">冲击容忍点数</label>
                <div class="layui-input-inline">
                    <input type="text" id="bumptimegap" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">制动清零标准值</label>
                <div class="layui-input-inline">
                    <input type="text" id="brakezerostandard" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">制动容忍点数</label>
                <div class="layui-input-inline">
                    <input type="text" id="brakelastingpoints" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">转向清零标准值</label>
                <div class="layui-input-inline">
                    <input type="text" id="steeringzerostandard" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">转向容忍点数</label>
                <div class="layui-input-inline">
                    <input type="text" id="steeringlastingpoints" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">油门清零标准值</label>
                <div class="layui-input-inline">
                    <input type="text" id="throttlezerostandard" placeholder="必填项" autocomplete="off" class="layui-input">
                </div>
            </div>
        </div>



    </form>
    <script>

        layui.use(['element', 'layer', 'form', 'laydate'], function () {
            var element = layui.element;
            var $ = layui.jquery;
            layer = layui.layer;

            var form = layui.form;


        });


    </script>

</body>
</html>

